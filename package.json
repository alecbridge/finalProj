    "@horizon/server": "^1.1.3",
    "npm-run-all": "^3.0.0",
    "express": "^4.14.0",
    "hjs-webpack": "^8.3.0",
    "leagueapi": "^0.4.21",
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.0",
    "react-fontawesome": "^1.1.0",
    "react-router": "^2.6.1",
    "react-star-rating-component": "^1.0.0",
    "style-loader": "^0.13.1"
  },
